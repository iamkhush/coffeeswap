{
  "name": "coffeeswap",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build public/bundle.js",
    "build": "babel server --out-dir build --presets=es2015 && webpack",
    "start": "cross-env NODE_ENV=production node ./build/main.js",
    "development": "cross-env NODE_ENV=development nodemon --exec babel-node --presets=es2015 ./server/main.js --watch server",
    "test": "nyc --branches -0.1 --lines -0.1 jest",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/iamkhush/coffeeswap.git"
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ]
  },
  "author": "Ankush Chadda",
  "license": "ISC",
  "description": "",
